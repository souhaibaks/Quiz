n        return {\r\n            quizzes\r\n        }\r\n    }\r\n}\r\n</script>\r\n\r\n<style>\r\n.container {\r\n    border: solid 1px black;\r\n    text-decoration: none;\r\n}\r\n\r\n</style>"],"mappings":";;;AAkBA,SAAQA,EAAE,QAAO,aAAY;AAC7B,SAAQC,UAAU,EAAEC,OAAO,QAAO,oBAAmB;AACrD,SAAQC,GAAG,EAAEC,SAAS,QAAO,KAAI;AAEjC,eAAe;EACXC,KAAKA,CAAA,EAAG;IACJ,MAAMC,OAAM,GAAIH,GAAG,CAAC,EAAE;IACtB,MAAMI,QAAO,GAAI,MAAAA,CAAA,KAAW;MACxB,MAAMC,KAAI,GAAI,MAAMN,OAAO,CAACD,UAAU,CAACD,EAAE,EAAE,SAAS,CAAC;MACrDQ,KAAK,CAACC,OAAO,CAAGC,GAAG,IAAI;QACnBJ,OAAO,CAACK,KAAK,CAACC,IAAI,CAAC;UACfC,EAAE,EAAEH,GAAG,CAACG,EAAE;UACV,GAAGH,GAAG,CAACI,IAAI,CAAC;QAChB,CAAC;MACL,CAAC,CAAC;IACN;IACAV,SAAS,CAACG,QAAQ;IAClB,OAAO;MACHD;IACJ;EACJ;AACJ","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}